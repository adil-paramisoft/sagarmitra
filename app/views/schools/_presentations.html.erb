<%= select_tag :filter, options_from_collection_for_select(@presentation_type, "id", "name"),:prompt => "All",:id=>'presentation'  %>
<div id='list'>
	<%= render :partial => 'presentations_rows' %>
</div>


<script>
    $( "#presentation" ).change(function() {
		$.ajax({
			url: "<%= presentation_school_path(@school) %>",
			data: { presentation_id: $( this ).val() },
			success:function(result){
		    $("#list").html(result);
		  }});
      	
    });

  </script>